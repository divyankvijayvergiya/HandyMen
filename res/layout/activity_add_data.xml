<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/cyan"
    android:finishOnTaskLaunch="true"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".AddData" >

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:text="Fill Details..."
        android:textColor="@color/blue"
        android:textSize="25sp" />

    <TextView
        android:id="@+id/a"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView1"
        android:layout_below="@+id/textView1"
        android:layout_marginTop="14dp"
        android:text="Name"
        android:textColor="@color/red"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/b"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/a"
        android:layout_below="@+id/a"
        android:layout_marginTop="14dp"
        android:text="Email"
        android:textColor="@color/red"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/d"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/c"
        android:layout_marginTop="23dp"
        android:text="Confirmpass"
        android:textColor="@color/red"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/e"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/d"
        android:layout_marginTop="14dp"
        android:text="Field"
        android:textColor="@color/red"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/f"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/e"
        android:layout_below="@+id/e"
        android:layout_marginTop="14dp"
        android:text="Charges"
        android:textColor="@color/red"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/h"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/g"
        android:layout_below="@+id/g"
        android:layout_marginTop="14dp"
        android:text="Address"
        android:textColor="@color/red"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/c"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/b"
        android:layout_marginTop="14dp"
        android:text="Password"
        android:textColor="@color/red"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/g"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/f"
        android:layout_below="@+id/f"
        android:layout_marginTop="14dp"
        android:text="Phone No."
        android:textColor="@color/red"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/i"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/h"
        android:layout_below="@+id/h"
        android:layout_marginTop="14dp"
        android:text="City"
        android:textColor="@color/red"
        android:textSize="18sp" />

    <EditText
        android:id="@+id/bb"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/b"
        android:layout_toRightOf="@+id/d"
        android:ems="10"
        android:hint="Enter E-mail"
        android:inputType="textEmailAddress" />

    <EditText
        android:id="@+id/cc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/c"
        android:layout_alignRight="@+id/bb"
        android:layout_toRightOf="@+id/d"
        android:ems="10"
        android:hint="Alphanumeric"
        android:inputType="textPassword" />

    <EditText
        android:id="@+id/dd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/e"
        android:layout_alignLeft="@+id/cc"
        android:layout_alignRight="@+id/cc"
        android:ems="10"
        android:hint="Re-enter Password"
        android:inputType="textPassword" />

    <EditText
        android:id="@+id/ee"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/e"
        android:layout_alignLeft="@+id/dd"
        android:layout_alignRight="@+id/dd"
        android:ems="10"
        android:hint="Enter Service"
        android:inputType="textPersonName" />

    <EditText
        android:id="@+id/ff"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/f"
        android:layout_alignRight="@+id/ee"
        android:layout_toRightOf="@+id/d"
        android:ems="10"
        android:hint="Per day cost"
        android:inputType="phone" />

    <EditText
        android:id="@+id/gg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/g"
        android:layout_alignRight="@+id/aa"
        android:layout_toRightOf="@+id/d"
        android:ems="10"
        android:hint="Contact"
        android:inputType="phone" />

    <EditText
        android:id="@+id/hh"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/h"
        android:layout_alignLeft="@+id/bb"
        android:ems="10"
        android:hint="Comm. Add."
        android:inputType="textPersonName" />

    <EditText
        android:id="@+id/ii"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/i"
        android:layout_alignLeft="@+id/hh"
        android:layout_alignRight="@+id/hh"
        android:ems="10"
        android:hint="State"
        android:inputType="textPersonName" />

    <EditText
        android:id="@+id/aa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView1"
        android:layout_toRightOf="@+id/d"
        android:hint="User Name"
        android:ems="10"
        android:inputType="textPersonName" />

    <Button
        android:id="@+id/addupload"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/i"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:background="@color/maroom"
        android:onClick="insert"
        android:text="Submit"
        android:textColor="@color/white"
        android:textSize="22sp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/c"
         android:textColor="@color/black"
        android:text="**Password Should Be Alphanumeric" />

</RelativeLayout>